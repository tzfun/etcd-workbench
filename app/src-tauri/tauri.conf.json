{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "Etcd Workbench"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "all": true
      },
      "os": {
        "all": true
      },
      "dialog": {
        "ask": true,
        "open": true,
        "save": true
      },
      "clipboard": {
        "all": false,
        "writeText": true
      },
      "process": {
        "relaunch": true
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "dialog": false,
