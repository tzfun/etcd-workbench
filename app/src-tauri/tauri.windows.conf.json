{
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "all": true
      },
      "os": {
        "all": true
      },
      "dialog": {
        "open": true,
        "save": true
      }
    },
    "windows": [
      {
        "label": "main",
        "title": "Etcd Workbench",
        "width": 1200,
        "height": 800,
        "minWidth": 900,
        "minHeight": 500,
        "center": true,
        "decorations": false,
        "transparent": false,
        "visible": false,
        "closable": true,
        "focus": false,
        "titleBarStyle": "Overlay"
      },
      {
        "label": "setting",
        "title": "Settings",
        "width": 1000,
        "height": 750,
        "resizable": false,
        "center": true,
        "url": "/?page=setting",
        "decorations": false,
        "titleBarStyle": "Overlay",
        "transparent": false,
        "visible": false,
        "focus": false
      },
      {
        "label": "splashscreen",
        "title": "Etcd Workbench",
        "width": 350,
        "height": 100,
        "center": true,
        "url": "splashscreen.html",
        "decorations": false,
        "resizable": false,
        "transparent": true,
        "visible": true
      }
    ],
    "macOSPrivateApi": true,
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.beifengtz.etcdworkbench",
      "icon": [
        "icons/windows/32x32.png",
        "icons/windows/128x128.png",
        "icons/windows/128x128@2x.png",
        "icons/windows/icon.ico"
      ]
    }
  }
}
